<Page x:Class="Project.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoginPage">

    <Grid Background="white">
        <TextBlock HorizontalAlignment="Left" Margin="262,98,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="250,206,0,0" TextWrapping="Wrap" x:Name="LoginText"  VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="262,261,0,0" TextWrapping="Wrap" x:Name="PassText"  VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="250,310,0,0" x:Name="LoginBtn" Click="LoginBtn_Click" VerticalAlignment="Top" Height="44" Width="176" RenderTransformOrigin="0.492,0.604"/>

    </Grid>
</Page>
